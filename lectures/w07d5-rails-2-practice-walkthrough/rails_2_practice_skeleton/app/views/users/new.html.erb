<h1>Sign Up</h1>

<%# watch out for extra spaces between quotesfor action %>
<form action="<%= users_url %>" method="POST">
  <input type="hidden" name="authenticy_token" value="<%= form_authenticity_token %>">
  <label>Username
    <input type="text" name="user[username]" value="<%= @user.username %>">
  </label>
  <label>Password
    <input type="text" name="user[password]">
  </label>
  <button>Sign Up</button>
</form>
