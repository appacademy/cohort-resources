User Index Page
<br>
<% if logged_in? %>
  <h1>Hello <%= current_user.username %></h1>
  <form action="<%= session_url %>" method="post">
    <input type="hidden" name="_method" value="DELETE">
    <input type="submit" value="logout!">
  </form>
<% else  %>
  <a href="<%= new_user_url %>">Go to new user page</a>
  <br>
  <a href="<%= new_session_url %>"> Go to Login page</a>
<% end  %>


<ul>
  <% @users.each do |user| %>
  <a href="<%= user_url(user) %>">User show page</a>
  <li>
    <%= render 'partial', banana: user %>
  </li>
  <% end %>
</ul>

